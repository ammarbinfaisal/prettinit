{
  "name": "prettinit",
  "version": "1.1.0",
  "description": "A simple cli tool for setting up .prettierrc file",
  "keywords": [
    "cli",
    "prettier",
    "prettierrc"
  ],
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "prettier": "prettier --write app.js ./lib/*.js",
    "test": "xo"
  },
  "author": "Ammar <binfaisal.ammar@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.1",
    "configstore": "^3.1.2",
    "inquirer": "^5.2.0",
    "json2yaml": "^1.1.0",
    "log-symbols": "^2.2.0",
    "meow": "^5.0.0"
  },
  "devDependencies": {
    "prettier": "^1.12.1",
    "xo": "^0.23.0"
  },
  "bin": {
    "prettinit": "./app.js"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ammarbinfaisal/prettinit.git"
  },
  "bugs": {
    "url": "https://github.com/ammarbinfaisal/prettinit/issues"
