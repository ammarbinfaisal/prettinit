{
  "name": "prettinit",
  "version": "0.4.0",
  "description": "A simple cli tool for setting up .prettierrc file",
  "keywords": [
    "cli",
    "prettier",
    "prettierrc"
  ],
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "prettier": "prettier --write app.js ./lib/*"
  },
  "author": "Ammar <binfaisal.ammar@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "boxen": "^1.3.0",
    "configstore": "^3.1.2",
    "inquirer": "^5.2.0",
    "json2yaml": "^1.1.0",
    "log-symbols": "^2.2.0",
    "meow": "^5.0.0"
  },
  "devDependencies": {
    "prettier": "^1.12.1"
  },
  "bin": {
    "prettinit": "./app.js"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ammarbinfaisal/prettinit.git"
  },
  "bugs": {
